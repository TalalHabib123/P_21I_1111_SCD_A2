{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "indent": "off",
    "no-tabs": "off",
    "camelcase": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "quote-props": "off",
    "radix": "off",
    "func-names": "off",
    "space-before-function-paren": "off",
    "spaced-comment": "off",
    "no-undef": "off",
    "no-console": "off",
    "prefer-template": "off",
    "newline-per-chained-call": "off",
    "no-shadow": "off",
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "max-len": "off",
    "dot-notation": "off",
    "one-var": "off",
    "one-var-declaration-per-line": "off",
    "semi": "off",
    "arrow-parens": "off",
    "no-useless-return": "off",
    "no-else-return": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "no-restricted-syntax": "off",
    "no-continue": "off",
    "no-loop-func": "off",
    "no-await-in-loop": "off",
    "no-return-assign": "off",
    "no-prototype-builtins": "off",
    "no-restricted-globals": "off",
    "no-undef-init": "off",
    "linebreak-style": "off",
    "no-unused-expressions": "off",
    "eol-last": "off",
    "no-var": "off",
    "arrow-body-style": "off",
    "import/order": "off"
  }
}